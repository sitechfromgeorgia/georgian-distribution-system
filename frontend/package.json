{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:turbo:debug": "next dev --turbo --debug",
    "dev:turbo:trace": "NEXT_BUILD_LOGS=true next dev --turbo --debug",
    "dev:docker": "next dev",
    "build": "next build",
    "build:turbo": "next build --experimental-build-mode",
    "build:debug": "next build --debug",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:docker": "next build",
    "start": "next start",
    "start:turbo": "next start --turbo",
    "start:docker": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "turbo": "turbo",
    "turbo:build": "turbo build",
    "turbo:dev": "turbo dev",
    "turbo:test": "turbo test",
    "turbo:lint": "turbo lint",
    "turbo:type-check": "turbo type-check",
    "perf:build": "cross-env SPEED_BENCHMARK=true next build",
    "perf:dev": "cross-env SPEED_BENCHMARK=true next dev --turbo",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:components": "vitest run src/components",
    "test:hooks": "vitest run src/hooks",
    "test:lib": "vitest run src/lib",
    "test:utils": "vitest run src/utils",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:smoke": "vitest run --reporter=basic --grep='smoke'",
    "test:critical": "vitest run --reporter=basic --grep='critical'",
    "test:regression": "vitest run --reporter=basic --grep='regression'",
    "test:full": "vitest run --reporter=basic",
    "test:api": "vitest run --reporter=basic src/tests/api",
    "test:auth": "vitest run --reporter=basic src/tests/auth",
    "test:realtime": "vitest run --reporter=basic src/tests/realtime",
    "test:roles": "vitest run --reporter=basic src/tests/roles",
    "test:business": "vitest run --reporter=basic src/tests/business",
    "test:admin": "vitest run --reporter=basic src/tests/admin",
    "test:restaurant": "vitest run --reporter=basic src/tests/restaurant",
    "test:driver": "vitest run --reporter=basic src/tests/driver",
    "test:demo": "vitest run --reporter=basic src/tests/demo",
    "test:lifecycle": "vitest run --reporter=basic src/tests/lifecycle",
    "test:pricing": "vitest run --reporter=basic src/tests/pricing",
    "test:integrity": "vitest run --reporter=basic src/tests/integrity",
    "test:comprehensive": "npm run test:full"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.77.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.548.0",
    "next": "^15.5.0",
    "next-themes": "^0.4.6",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^2.12.7",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "winston": "^3.17.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.0.1",
    "@sentry/nextjs": "^8.45.1",
    "@supabase/supabase-js": "^2.77.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9",
    "eslint-config-next": "^15.0.0",
    "happy-dom": "^16.7.2",
    "puppeteer": "^24.27.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "turbo": "^2.4.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^2.1.8",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-merge": "^6.0.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
